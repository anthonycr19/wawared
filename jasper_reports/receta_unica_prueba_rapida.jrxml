	<queryString>
		<![CDATA[select upper(paciente.nombres || ' ' || paciente.apellido_paterno || ' ' || paciente.apellido_materno) as nombre_completo, extract(YEAR from age(now(), paciente.fecha_nacimiento))::int as edad,
historia_clinica.numero as hc,
to_char(control.atencion_fecha, 'DD/MM/YYYY') as fecha_atencion, control.indicacion_hierro,
upper(paciente.tipo_documento) || ': ' || paciente.numero_documento as documento, paciente.seguro_sis, paciente.codigo_afiliacion


from controles_control as control
join pacientes_paciente as paciente on control.paciente_id = paciente.id
left join pacientes_historiaclinica as historia_clinica on control.paciente_id = historia_clinica.paciente_id and control.establecimiento_id = historia_clinica.establecimiento_id
where control.id = $P{control_id}::int]]>
	</queryString>
